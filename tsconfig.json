{
    "compilerOptions": {
        "target": "es2016",
        "module": "commonjs",
        "declaration": false,
        "sourceMap": true,
        "outDir": "dist",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "skipLibCheck": true,
        "lib": [
            "ESNext",
            "DOM",
        ],
    }
}


