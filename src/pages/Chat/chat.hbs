<div class="chat__body">
    <div class="column column__left">
        <nav class="chat__nav">
            {{{profile}}}
            {{{search}}}
        </nav>
        <div class="scroll-y">

        </div>
    </div>
    <div class="column column__right">
        <main class="chat__container">
            <header class="chat__head">
                {{{usericon}}}
                <label class="chat__head__name">John Doe</label>
            </header>

            <footer class="chat__composer">
                <div class="composer">
                    {{messageinput}}
                    {{sendbutton}}
                </div>
            </footer>
        </main>
    </div>
</div>
