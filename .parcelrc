{
    "extends": "@parcel/config-default",
    "transformers": {
        "*.hbs": [
            "@anikin/parcel-transformer-handlebars-precompile"
        ],
        "blob-url:*": [
            "@parcel/transformer-inline"
        ]
    },
    "optimizers": {
        "blob-url:*": [
            "@parcel/optimizer-blob-url"
        ]
    }
}
